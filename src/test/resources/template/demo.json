{
    "name": "demo-template",
    "description": "Demonstrates workflow steps and passing around of input/output",
    "user_inputs": {
        "knn_index_name": "my-knn-index"
    },
    "workflows": {
        "demo": {
            "nodes": [
                {
                    "id": "fetch_model",
                    "type": "demo_delay_3",
                    "inputs": {
                        "ingest_key": "ingest_value"
                    }
                },
                {
                    "id": "register_model",
                    "type": "register_model",
                    "inputs": {
                        "name": "openAI-gpt-3.5-turbo",
                        "function_name": "remote",
                        "description": "test model",
                        "connector_id": "uDna54oB76l1MtYJF84U"
                    }
                },
                {
                    "id": "deploy_model",
                    "type": "deploy_model",
                    "inputs": {
                        "model_id": "abc"
                    }
                }
            ],
            "edges": [
                {
                    "source": "fetch_model",
                    "dest": "deploy_model"
                }
            ]
        }
    }
}
